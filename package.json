{
  "name": "vidfast-m3u8-extractor",
  "version": "1.0.0",
  "description": "VidFast M3U8 URL extractor using Playwright",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:proxy": "node proxy-server.js",
    "start:advanced-proxy": "node advanced-proxy-server.js",
    "start:nginx-proxy": "node nginx-style-proxy.js",
    "start:both": "concurrently \"npm:start\" \"npm:start:proxy\"",
    "start:all": "concurrently \"npm:start\" \"npm:start:proxy\" \"npm:start:advanced-proxy\" \"npm:start:nginx-proxy\"",
    "install-browsers": "npx playwright install firefox"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.58.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
